RELATION Movies {
	title : STRING
	year : INTEGER
	length : INTEGER
	genre : STRING
	studioname : STRING
	producerid : INTEGER
}

RELATION MovieStar {
	name : STRING
	address : STRING
	gender : CHAR
}

RELATION StarsIn {
	movietitle : STRING
	movieyear : INTEGER
	starname : STRING
}

RELATION MovieExec {
	name : STRING
	address : STRING
	certid : INTEGER
	networth : INTEGER
}

RELATION Studio {
	name : STRING
	address : STRING
	presid : CHAR
}